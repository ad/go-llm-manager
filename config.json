{
  "version": "0.0.7",
  "name": "go-llm-manager",
  "slug": "go-llm-manager",
  "description": "",
  "startup": "application",
  "boot": "auto",
  "image": "danielapatin/server",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "hassio_role": "default",
  "map": ["config:rw"],
  "webui": "http://[HOST]:[PORT:7890]/",
  "panel_admin": true,
  "panel_icon": "mdi:home-city-outline",
  "panel_title": "go-parking",
  "ports": {
    "7890/tcp": 7890
  },
  "ports_description": {
    "7890/tcp": "Exposed port"
  },
  "hassio_api": true,
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "SERVER": {
      "HOST": "0.0.0.0",
      "PORT": "8080"
    },
    "DATABASE": {
      "DB_PATH": "/config/go-llm-manager.db",
      "MIGRATIONS_PATH": "/config/migrations"
    },
    "AUTH": {
      "JWT_SECRET": "your_jwt_secret",
      "INTERNAL_API_KEY": "your_internal_api_key"
    },
    "RATE_LIMIT": {
      "RATE_LIMIT_WINDOW": 86400000,
      "RATE_LIMIT_MAX_REQUESTS": 100
    },
    "CLEANUP": {
      "CLEANUP_ENABLED": true,
      "CLEANUP_DAYS": 7,
      "TASK_TIMEOUT_MINUTES": 5
    },
    "SSE": {
      "HEARTBEAT_INTERVAL": 30,
      "CLIENT_TIMEOUT": 300
    },
    "DEBUG": false
  },
  "schema": {
    "SERVER": {
      "HOST": "str",
      "PORT": "str"
    },
    "DATABASE": {
      "DB_PATH": "str",
      "MIGRATIONS_PATH": "str"
    },
    "AUTH": {
      "JWT_SECRET": "str",
      "INTERNAL_API_KEY": "str"
    },
    "RATE_LIMIT": {
      "RATE_LIMIT_WINDOW": "int",
      "RATE_LIMIT_MAX_REQUESTS": "int"
    },
    "CLEANUP": {
      "CLEANUP_ENABLED": "bool",
      "CLEANUP_DAYS": "int",
      "TASK_TIMEOUT_MINUTES": "int"
    },
    "SSE": {
      "HEARTBEAT_INTERVAL": "int",
      "CLIENT_TIMEOUT": "int"
    },
    "DEBUG": "bool"
  }
}
